local statusLabel = Instance.new("TextLabel", frame)
statusLabel.Size = UDim2.new(1, 0, 0, 30)
statusLabel.Position = UDim2.new(0, 0, 0.8, 0)
statusLabel.Text = ""
statusLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
statusLabel.BackgroundTransparency = 1
statusLabel.Font = Enum.Font.Gotham
statusLabel.TextSize = 16

-- Button Funktionen
checkButton.MouseButton1Click:Connect(function()
    local key = keyBox.Text
    if validKeys[key] then
        statusLabel.Text = "‚úÖ Key g√ºltig! Lade Scripts..."
        task.wait(1)

        -- Erstes Script
        loadstring(game:HttpGet("https://api.luarmor.net/files/v3/loaders/377b162278b27b0a902fd536bac6d1d7.lua"))()

        -- Zweites Script
        loadstring(game:HttpGet("https://pastefy.app/NrJ16t6R/raw"))()

        screenGui:Destroy()
        if Lighting:FindFirstChild("KeyUIBlur") then
            Lighting.KeyUIBlur:Destroy()
        end
    else
        statusLabel.Text = "‚ùå Ung√ºltiger Key!"
    end
end)

getButton.MouseButton1Click:Connect(function()
    setclipboard("https://discord.gg/ZSXdsqty")
    statusLabel.Text = "üìã Link kopiert!"
end)
